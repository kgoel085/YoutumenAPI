{
    "Global":{
        "url": "https://www.googleapis.com/youtube/v3/"
    },
    "Endpoints":{
        "excludeFields":{
            "kind": null,
            "etag": null,
            "items": ["kind", "etag"]
        },
        "locations":{
            "params": {"part":"snippet"},
            "endpoint": "i18nRegions"
        },
        "trending":{
            "params": {
                "chart": "mostpopular",
                "part":"snippet",
                "maxResults": 15, 
                "fields": "nextPageToken,pageInfo,items(snippet/channelId)"
            },
            "common": ["chart", "maxResults"],
            "endpoint":"videos"
        },
        "channel":{
            "params": {
                "part": "snippet", 
                "fields": "items(id,contentDetails,snippet(title, thumbnails, description))", 
                "id": null,
                "categoryId": null,
                "forUsername": null,
                "managedByMe": null,
                "mine": null,
                "mySubscribers": null,
                "onBehalfOfContentOwner": null,
                "maxResults": 10,
                "pageToken": null
            },
            "endpoint": "channels",
            "common":["part", "maxResults"]
        },
        "playlist":{
            "params":{
                "part": "snippet", 
                "maxResults": 8, 
                "fields": "items(snippet(resourceId/videoId))", 
                "playlistId": null,
                "channelId": null,
                "id": null,
                "mine": null,
                "onBehalfOfContentOwner": null,
                "pageToken": null
            },
            "common":["part", "maxResults"],
            "endpoint": "playlistItems"
        },
        "playlistItems":{
            "params":{
                "part": "snippet",
                "id": null,
                "playlistId": null,
                "maxResults": 10,
                "onBehalfOfContentOwner": null,
                "pageToken": null,
                "videoId": null
            },
            "common":[],
            "endpoint": "playlistItems"
        },
        "video":{
            "params":{
                "part": "snippet", 
                "chart": "mostPopular", 
                "id": null, 
                "myRating": ["like", "dislike"],
                "maxResults": 10,
                "pageToken": null,
                "regionCode": null,
                "videoCategoryId": null,
                "fields":null
            },
            "common":["part", "maxResults"],
            "endpoint":"videos"
        },
        "search":{
           "params":{
                "part": "snippet", 
                "maxResults": 15, 
                "order": "relevance",
                "q": null, 
                "type":"video", 
                "videoType":"any",
                "publishedAfter":null,
                "publishedBefore":null,
                "videoDuration":null
            },
            "common": ["part", "maxResults", "type", "videoType"],
            "endpoint":"search"
        }
    }   
}